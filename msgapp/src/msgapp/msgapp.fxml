<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="msgapp.Main" spacing="20">
    <Label fx:id="label1" text="Choice Menu"/>
    <TextField text="Choose between Server and Client"/>
    <Button fx:id="button_server" text="Server" onAction="#reactToClickServer"/>
    <Button fx:id="button_client" text="Client" onAction="#reactToClickClient"/>
</VBox>
